{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "no-unused-variable": true,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 500]
  }
}
